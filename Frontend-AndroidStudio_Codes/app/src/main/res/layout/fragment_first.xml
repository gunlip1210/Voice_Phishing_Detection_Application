<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/android_lar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="@color/blue">

    <TextView
        android:id="@+id/textViewFragment1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="통화녹음 분석"
        android:fontFamily="@font/notosans_kr_medium"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:textSize="34sp"
        android:textColor="@color/white"/>

    <TextView
        android:id="@+id/textViewFragment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="통화 녹음본을 업로드하면"
        android:fontFamily="@font/notosans_kr_extralight"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:textSize="15sp"
        android:textColor="@color/white"/>

    <TextView
        android:id="@+id/textViewFragment3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="135dp"
        android:text="보이스피싱 여부를 분석해드려요."
        android:fontFamily="@font/notosans_kr_extralight"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:textSize="15sp"
        android:textColor="@color/white"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/uploadButton"
            android:layout_width="match_parent"
            android:layout_height="680dp"
            android:layout_marginTop="180dp"
            android:src="@drawable/upload_button"
            android:contentDescription="uploadButton"
            android:scaleType="fitCenter"
            android:adjustViewBounds="true"
            android:background="@android:color/transparent" />

    </FrameLayout>

    <ImageButton
        android:id="@+id/buttonGoBack1"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:padding="0dp"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true"
        android:contentDescription="buttonGoBack1"
        android:src="@drawable/button_go_back1"
        android:background="@android:color/transparent"/>

</RelativeLayout>
